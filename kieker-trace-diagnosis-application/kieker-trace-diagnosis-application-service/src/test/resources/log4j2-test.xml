<?xml version="1.0" encoding="UTF-8"?>

<Configuration status="WARN">

	<Appenders>
		<Console name="console" target="SYSTEM_OUT">
			<PatternLayout pattern="%d{HH:mm:ss} %-5level %logger{36} - %msg%rEx{0}%n" />
		</Console>
	</Appenders>

	<Loggers>
		<Root level="all">
			<AppenderRef ref="console" />
		</Root>
	</Loggers>

</Configuration>